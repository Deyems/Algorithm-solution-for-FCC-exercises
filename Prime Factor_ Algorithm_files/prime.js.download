var MAXLIMIT=Math.pow(10,250);function IUgcd(){var a=parseInt(document.myForm.txtInputA.value);var d=parseInt(document.myForm.txtInputB.value);var c=gcd(a,d);s="gcd("+a+", "+d+") = "+c;document.myForm.txtResult.value=s}function IUlcm(){var a=parseInt(document.myForm.txtInputA.value);var d=parseInt(document.myForm.txtInputB.value);var c=lcm(a,d);s="lcm("+a+", "+d+") = "+c;document.myForm.txtResult.value=s}function IUQuotientRemainder(){var a=parseInt(document.myForm.txtInputA.value);var e=parseInt(document.myForm.txtInputB.value);var d=IntDiv(a,e);var c=Remainder(a,e);s=a+" = "+d+" x "+e+" + "+c;document.myForm.txtResult.value=s}function IUDivisibilityPrimeFactorization(){var d=document.myForm.txtInput.value;var c=IsPrime(d);if(c===false){s="Input must be positive integer larger than 1 and less than "+MAXLIMIT}else{if(c==1){s=d+" is a Prime number.\nThe only factors are 1 and "+d+" itself."}else{s=d+" is a Composite number.\n\n";s+="Prime factorization: \n";s+=PrimeFactorization(d)+"\n\n";var a=FindDivisors(d);s+="There are "+a.length+" divisors. \n";s+="Divisors of "+d+" are "+PrintArray(a)+"\n"}}document.myForm.txtResult.value=s}function UIErastosthenesSieve(){var d=document.myForm.txtInput1.value;var c=document.myForm.txtInput2.value;var a=ErastosthenesSieve(d,c);if(a.length==0){s="There is no prime number between "+d+" and "+c}else{if(a===false){s="Input must be positive integer larger than 1 and less than 1000000"}else{s="There are "+a.length+" prime numbers between "+d+" and "+c+":\n";s+=PrintArray(a)+"\n"}}document.myForm.txtResult.value=s}function IUDivisibility(){var c=document.myForm1.txtInput1.value;var a=FindDivisors(c);if(a===false){txt="Input must be positive integer larger than 1 and less than "+MAXLIMIT}else{txt=c+" has "+a.length+" divisors: \n";txt+=PrintArray(a)}document.myForm1.txtResult1.value=txt}function IUPrimeFactorization(){var c=document.myForm.txtInput.value;var a=PrimeFactorization(c);if(a===false){s="Input must be positive integer larger than 1 and less than "+MAXLIMIT}else{s=a}document.myForm.txtResult.value=s}function IUIsPrime(){var c=document.myForm.txtInput.value;var a=IsPrime(c);if(a===false){s="Input must be positive integer larger than 1 and less than "+MAXLIMIT}else{if(a==1){s=c+" is a Prime number"}else{s=c+" is a Composite number"}}document.myForm.txtResult.value=s}function ErastosthenesSieve(f,e){f=parseInt(f);e=parseInt(e);var a=0;var d=new Array();if(e<f){var c=f;f=e;e=c}for(var h=f;h<=e;h++){i=IsPrime(h);if(i==1){d[a]=h;a++}}return d}function IsPrime(c){if(isPosInteger(c)&c>=2&c<MAXLIMIT){Limit=Math.floor(Math.sqrt(c));for(var a=Limit;a>=2;a--){if(c%a==0){break}}return a}else{return false}}function PrimeFactorization(l){if(isPosInteger(l)&l>=2&l<MAXLIMIT){var k=2;var f=0;var c=new Array();var a=l+" = ";while(l>=k*k){if(l%k==0){c[f]=k;l=l/k;f++}else{k++}}c[f]=l;var h=0;for(var d=0;d<c.length;d++){b=c[d];h++;if(c[d]!=c[d+1]){if(d==c.length-1){if(h>1){a+=b+"^"+h}else{a+=b}}else{if(h>1){a+=b+"^"+h+" * "}else{a+=b+" * "}}h=0}}return a}else{return false}}function PrimeFactorization0(a){if(isPosInteger(a)&a>=2&a<MAXLIMIT){p=2;txt=a+" = ";while(a>=p*p){if(a%p==0){txt+=p+" * ";a=a/p}else{p++}}txt+=a;return txt}else{return false}}function FindDivisors(d){if(isPosInteger(d)&d<MAXLIMIT){var a=new Array();i=0;Limit=Math.floor(Math.sqrt(d));for(var c=1;c<=Limit;c++){if(d%c==0){a[i]=c;i++}}m=i--;if(IntDiv(d,a[m-1])==a[m-1]){n=2*m-1;for(j=0;j<m-1;j++){a[m+j]=IntDiv(d,a[m-j-2])}}else{n=2*m;for(j=0;j<m;j++){a[m+j]=IntDiv(d,a[m-j-1])}}return a}else{return false}}function PrintArray(c){var a="";for(var d=0;d<c.length-1;d++){a+=c[d]+","}a+=c[c.length-1];return a}function isPosInteger(c){inputStr=c.toString();for(var a=0;a<inputStr.length;a++){var d=inputStr.charAt(a);if(d<"0"||d>"9"){return false}}return true}function IsDivisible(a,c){return(a%c==0)}function IntDiv(a,c){return Math.floor(a/c)}function Remainder(a,c){return a%c}function IsCongruent(e,d,c){return IsDivisible(e-d,c)}function gcd(a,d){if(d==0){return a}else{var c=1;while(c!=0){c=a%d;a=d;d=c}return a}}function lcm(a,c){g=gcd(a,c);if(g==0){return 0}else{return a*c/g}};